<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student" class="com.project.springXmlBased.entitys.student.Student" c:_0="Sasha" c:_1="25"
          p:subject-ref="subjectMath"
          p:teacher-ref="teacherMath"/>

    <bean id="subjectMath" class="com.project.springXmlBased.entitys.subject.impl.Math"/>

    <bean id="subjectPhysic" class="com.project.springXmlBased.entitys.subject.impl.Physics"/>

    <bean id="teacherMath" class="com.project.springXmlBased.entitys.teacher.impl.MathTeacher"/>

    <bean id="teacherPhysic" class="com.project.springXmlBased.entitys.teacher.impl.PhysicTeacher"/>

</beans>